{
  "ociVersion": "1.0.0",
  "process": {
    "terminal": false,
    "user": {
      "uid": -2,
      "gid": -2
    },
    "args": [
      "node",
      "<replace me>"
    ],
    "env": [
      "PATH=/bin:/usr/bin",
      "TERM=xterm",
      "_ALICE_IPC_PATH=<replace me>",
      "_ALICE_CODE_PATH=<replace me>",
      "_ALICE_WORKER_CREDENTIAL=<replace me>",
      "_ALICE_FUNC_HANDLER=with_initializer.handler",
      "_ALICE_FUNC_INITIALIZER=with_initializer.initializer"
    ],
    "noNewPrivileges": true
  },
  "root": {
    "path": "rootfs",
    "readonly": true
  },
  "linux": {
    "resources": {
      "memory": {
        "limit": 536870912
      },
      "cpu": {
        "shares": 1024,
        "quota": 1000000,
        "period": 1000000
      }
    },
    "seccomp": {
      "defaultAction": "SCMP_ACT_ALLOW",
      "syscalls": [
        {
          "names": [
            "getcwd",
            "chmod"
          ],
          "action": "SCMP_ACT_ERRNO"
        }
      ]
    }
  },
  "turf": {
    "runtime": "nodejs-v16",
    "code": "code"
  }
}
